package Geometria;

public class Rectangulo extends Figura implements IFigura {
	
	public int base;
	public int altura;

	public Rectangulo(int base, int altura) {
		super();
		this.base = base;
		this.altura = altura;
	}

	@Override
	public int dibujar() {
		return 0;
	}

	@Override
	public String obtenerNombreFigura() {
		return null;
	}

	@Override
	public double calcularArea() {
		return 0;
	}

	@Override
	public double calcularPerimetro() {
		return 0;
	}

}
