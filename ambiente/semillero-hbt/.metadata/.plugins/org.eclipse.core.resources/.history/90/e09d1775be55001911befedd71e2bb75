package Geometria;

public abstract class Figura {

	public int lados;
	public String nombre;
	
	public int getLados() {
		return lados;
	}

	public void setLados(int lados) {
		this.lados = lados;
	}

	public String getNombre() {
		return nombre;
	}

	public void setNombre(String nombre) {
		this.nombre = nombre;
	}

	public abstract int dibujar();
}
